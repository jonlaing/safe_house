{
  "name": "SafeHouseApp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start"
  },
  "dependencies": {
    "@exponent/react-native-navigator": "^0.4.3",
    "keypair": "^1.0.0",
    "moment": "^2.13.0",
    "react": "^0.14.8",
    "react-native": "^0.24.1",
    "react-native-device-info": "^0.9.3",
    "react-native-i18n": "0.0.8",
    "react-native-keyboard-spacer": "^0.1.6",
    "react-native-vector-icons": "^1.3.4"
  },
  "eslintConfig": {
    "plugins": [
      "react"
    ],
    "env": {
      "node": true,
      "es6": true
    },
    "ecmaFeatures": {
      "jsx": true,
      "modules": true
    },
    "rules": {
      "camelcase": [
        "error",
        {
          "properties": "never"
        }
      ],
      "no-use-before-define": 0,
      "no-underscore-dangle": 0,
      "quotes": [
        0,
        "single"
      ]
    }
  }
}
